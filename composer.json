{
    "name": "jadu/continuum-common",
    "type": "library",
    "require": {
        "php": ">=5.5.9",
        "doctrine/collections": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Jadu\\ContinuumCommon\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "phpunit/phpunit": "~4.8",
        "mockery/mockery": "~0.9.0",
        "jadu/php-style": "^1.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Jadu\\ContinuumCommon\\Tests\\": "tests/",
            "": "src"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.5"
        }
    }
}

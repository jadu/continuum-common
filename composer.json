{
    "name": "jadu/continuum-common",
    "type": "library",
    "repositories": [
        {
            "type": "composer",
            "url": "https://toran-proxy.hq.jadu.net/repo/private/"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "doctrine/collections": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Jadu\\ContinuumCommon\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "phpunit/phpunit": "~4.8",
        "mockery/mockery": "~0.9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Jadu\\ContinuumCommon\\Tests\\": "tests/",
            "": "src"
        }
    },
    "config": {
        "platform": {
            "php": "5.5.9"
        }
    }
}
